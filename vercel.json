{
  "version": 2,
  "framework": "none",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/klaso/browser",
  "routes": [
    {
      "src": "/(.*\\.(?:css|js|png|jpg|jpeg|gif|svg|ico|woff2?|ttf|map))",
      "dest": "/$1",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


